{
  "name": "aa-rectangle-javascript",
  "version": "0.1.1",
  "description": "A Javascript implementation of an Axis-Aligned Rectangle with extended features.",
  "scripts": {
    "test": "standard --fix && nyc mocha"
  },
  "main": "lib/aa-rectangle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauloddr/aa-rectangle-javascript.git"
  },
  "keywords": [
    "axis-aligned",
    "rectangle",
    "aabb",
    "2d",
    "hitbox",
    "hurtbox",
    "collision",
    "game"
  ],
  "author": "@pauloddr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pauloddr/aa-rectangle-javascript/issues"
  },
  "homepage": "https://github.com/pauloddr/aa-rectangle-javascript#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "performance-now": "2.1.0",
    "standard": "*"
  },
  "standard": {
    "env": "mocha",
    "ignore": [
      "node_modules/**"
    ]
  },
  "nyc": {
    "include": [
      "index.js",
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ]
  }
}
